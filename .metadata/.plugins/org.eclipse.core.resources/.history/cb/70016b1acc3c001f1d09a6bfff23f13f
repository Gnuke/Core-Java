
public class Practice01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int count = 0;
		int line = 1;
		
		for( int i = 65 ; i<=90 ; i++ ) {
			if( line %2 ==0 ) {
				System.out.println( (char)(i+32) + "\t" );
			}
			else {
				System.out.printf( "%c\t", (char)i );
			}
			
			count++;
			
			if( count % 5 == 0 ) {
				System.out.println(); line++;
			}
			
			i++;
		}
	}

}
