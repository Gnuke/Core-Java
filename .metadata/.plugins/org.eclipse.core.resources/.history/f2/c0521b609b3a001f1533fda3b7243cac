
public class LoopDemo {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// A ~ Z 까지 출력하기
		
		int i = 65;; // initialization
		
		for( ; i<=90;  ) {
			
			System.out.printf( "%c\t", (char)i );
			
			i++;
			//i -= 3; // i = i-3;
		}
		
		System.out.println();
		
	}

}
