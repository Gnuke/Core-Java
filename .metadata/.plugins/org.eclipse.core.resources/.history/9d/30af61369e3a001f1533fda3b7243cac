
public class Practice3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 1 ~ 100까지 자연수 중 4의 배수는 몇개인가
		
		int i = 1;
		int count = 0;
		
		for(;i<=100;) {
			if( i%4 == 0 ) {
				count++;
			}
			i++;
		}
		
		System.out.printf( "갯수는 : %d개\n" , count );
		
	}

}
