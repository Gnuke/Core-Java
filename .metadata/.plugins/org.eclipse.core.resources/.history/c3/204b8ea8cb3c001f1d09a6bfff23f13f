
public class Practice01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int count = 1;
		int line = 0;
		
		for( int i = 65 ; i<=90 ; i++ ) {
			System.out.printf( "%c\t", (char)i );
			
			if( count % 5 == 0 ) {
				System.out.println();
			}
			
			count++;
			i++;
		}
	}

}
